    "macros": {
      "S3bqu5t7QhV5DuCn": {
        "command": "const chaosRoll = new Roll(\"d12\", {});\nawait chaosRoll.evaluate();\nchaosRoll.toMessage();\nconst rollValue = chaosRoll.total;\n\nconst dmgTypes = [\n  \"bludgeoning\", \"piercing\", \"slashing\", \"cold\",\n  \"corrosion\", \"fire\", \"lightning\", \"poison\",\n  \"psychic\", \"radiant\", \"umbral\", \"true\"\n];\nitem.system.formulas.onyYxitxsGsPt92t.type = dmgTypes[rollValue - 1];\nconst againstStatus = _status(rollValue);\nif (againstStatus) item.system.againstStatuses.chaosBomb = againstStatus;\n\nif (rollValue === 10) {\n  item.system.enhancements[\"M9OPJ2gqoZzohBuh\"].number = 1;\n}\n\nfunction _status(rollValue) {\n  const status = new DC20.AgainstStatus();\n  switch(rollValue) {\n    case 1: \n      status.id = \"movement\";\n      break;\n    case 3: \n      status.id = \"bleeding\";\n      status.forOneMinute = true;\n      break;\n    case 4: \n      status.id = \"slowed\";\n      status.forXRounds = 1;\n      break;\n    case 5: \n      status.id = \"hindered\";\n      status.forXRounds = 1;\n      break;\n    case 6: \n      status.id = \"burning\";\n      status.forOneMinute = true;\n      break;\n    case 7: \n      status.id = \"stunned\";\n      status.untilTargetNextTurnEnd = true;\n      break;\n    case 8: \n      status.id = \"impaired\";\n      status.forXRounds = 1;\n      break;\n    case 9: \n      status.id = \"dazed\";\n      status.forXRounds = 1;\n      break;\n    case 11: \n      status.id = \"doomed\";\n      status.forXRounds = 1;\n      break;\n    case 12: \n      status.id = \"exposed\";\n      status.forXRounds = 1;\n      break;\n  }\n  if (!status.id) return null;\n  return status;\n}",
        "trigger": "preItemRoll",
        "disabled": false,
        "name": "Roll Chaos Bolt",
        "title": "",
        "global": false
      },
      "4Df9Wf1Deb5MhgmY": {
        "command": "const value = await DC20.dialog.SimplePopup.input(\"Provide Spell Attack value\");\nif (!value) return;\n\nawait item.update({\n  [\"system.rollMenu.free\"]: true,\n  [\"system.rollMenu.ignoreMCP\"]: true,\n});\nawait item.roll({customFormula: `d0 + ${value}`, quickRoll: true});",
