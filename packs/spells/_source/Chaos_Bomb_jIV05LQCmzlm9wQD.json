      "Uwa9iffipJMFjXdU": {
        "command": "const value = await DC20.dialog.SimplePopup.input(\"Fate Roll Value\");\nconst fateRoll = parseInt(value);\nif (!fateRoll) return;\n\nconst updateData = {\n  system: {\n    rollMenu: {free: true},\n    actionType: \"attack\",\n    formulas: {chaosBomb: _formula(fateRoll)},\n    againstStatuses: {},\n    rollRequests: {chaosBomb: _rollRequest()}\n  }\n}\nconst againstStatus = _status(fateRoll);\nif (againstStatus) updateData.system.againstStatuses.chaosBomb = againstStatus;\n\n// Radiant need its own effect\nif (fateRoll === 10) {\n  game.dc20rpg.effects.createEffectOn(_radiantEffectData(), item);\n}\n\nawait item.update(updateData);\nawait item.roll();\nconst chaosBombRadiant = item.getEffectByKey(\"chaosBombRadiant\");\nif (chaosBombRadiant) chaosBombRadiant.delete();\nitem.update({\n  [\"system.actionType\"]: \"check\",\n  [\"system.formulas.-=chaosBomb\"]: null,\n  [\"system.againstStatuses.-=chaosBomb\"]: null,\n  [\"system.rollRequests.-=chaosBomb\"]: null, \n})\n\nfunction _formula(fateRoll) {\n  const dmgTypes = [\n    \"bludgeoning\", \"piercing\", \"slashing\", \"cold\",\n    \"corrosion\", \"fire\", \"lightning\", \"poison\",\n    \"psychic\", \"radiant\", \"umbral\", \"true\"\n  ];\n  const formula = new DC20.Formula();\n  formula.formula = \"1\";\n  formula.type = dmgTypes[fateRoll - 1];\n  return formula;\n}\n\nfunction _status(fateRoll) {\n  const status = new DC20.AgainstStatus();\n  switch(fateRoll) {\n    case 1: \n      status.id = \"movement\";\n      break;\n    case 3: \n      status.id = \"bleeding\";\n      status.forOneMinute = true;\n      break;\n    case 4: \n      status.id = \"slowed\";\n      status.untilYourNextTurnEnd = true;\n      break;\n    case 5: \n      status.id = \"hindered\";\n      status.untilYourNextTurnEnd = true;\n      break;\n    case 6: \n      status.id = \"burning\";\n      status.forOneMinute = true;\n      break;\n    case 7: \n      status.id = \"stunned\";\n      status.untilYourNextTurnEnd = true;\n      break;\n    case 8: \n      status.id = \"impaired\";\n      status.untilYourNextTurnEnd = true;\n      break;\n    case 9: \n      status.id = \"dazed\";\n      status.untilYourNextTurnEnd = true;\n      break;\n    case 11: \n      status.id = \"doomed\";\n      status.untilYourNextTurnEnd = true;\n      break;\n    case 12: \n      status.id = \"exposed\";\n      status.untilYourNextTurnEnd = true;\n      break;\n  }\n  if (!status.id) return null;\n  return status;\n}\n\nfunction _rollRequest() {\n  const rollRequest = new DC20.RollRequest();\n  rollRequest.saveKey = \"int\";\n}\n\nfunction _radiantEffectData() {\n  const changes = [];\n  changes.push({\n    key: \"system.dynamicRollModifier.againstYou.martial.melee\",\n    mode: 2,\n    value: '\"label\": \"Chaos Bomb - Radiant\", \"modifier\": \"+ 1d4\"'\n  });\n  changes.push({\n    key: \"system.dynamicRollModifier.againstYou.martial.ranged\",\n    mode: 2,\n    value: '\"label\": \"Chaos Bomb - Radiant\", \"modifier\": \"+ 1d4\"'\n  });\n  changes.push({\n    key: \"system.dynamicRollModifier.againstYou.martial.area\",\n    mode: 2,\n    value: '\"label\": \"Chaos Bomb - Radiant\", \"modifier\": \"+ 1d4\"'\n  });\n  changes.push({\n    key: \"system.dynamicRollModifier.againstYou.spell.melee\",\n    mode: 2,\n    value: '\"label\": \"Chaos Bomb - Radiant\", \"modifier\": \"+ 1d4\"'\n  });\n  changes.push({\n    key: \"system.dynamicRollModifier.againstYou.spell.ranged\",\n    mode: 2,\n    value: '\"label\": \"Chaos Bomb - Radiant\", \"modifier\": \"+ 1d4\"'\n  });\n  changes.push({\n    key: \"system.dynamicRollModifier.againstYou.spell.area\",\n    mode: 2,\n    value: '\"label\": \"Chaos Bomb - Radiant\", \"modifier\": \"+ 1d4\"'\n  });\n\n  return {\n    flags: {dc20rpg: {}},\n    changes: changes,\n    img: \"icons/magic/light/explosion-star-teal.webp\",\n    name: \"Chaos Bomb - Radiant\",\n    description: \"Attacks made against you have a d4 bonus for 1 round.\",\n    duration: {\n      rounds: 1, \n    },\n    system: {\n      duration: {\n        useCounter: true,\n        onTimeEnd: \"delete\"\n      },\n      addToChat: true,\n      itemKey: \"chaosBombRadiant\"\n    },\n    transfer: false,\n  }\n}",
