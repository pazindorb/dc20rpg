    "macros": {
      "Xw63KTRMLdajHFHg": {
        "command": "const effect = item.getEffectByKey(\"bane\");\nif (!effect) return;\n\nconst enhancements = item.enhancements.active;\nconst full = enhancements.get(\"6I4hf5EzKEwUw3Ki\");\nconst greater = enhancements.get(\"2V0UDp6caiOiYgJj\");\n\nconst fromEnh = greater?.number || 0;\nlet diceValue = Math.min(6 + 2 * fromEnh, 12);\nconst dice = `d${diceValue}`;\n\nfor (const change of effect.changes) {\n  change.value = change.value.replace(\"#DICE#\", dice);\n}\n\nif (full) {\n  effect.changes.push({\n    key: \"system.dynamicRollModifier.onYou.saves.mig\",\n    mode: 2,\n    value: `\"value\": 0, \"label\": \"Bane\", \"modifier\": \"- 1${dice}\"`\n  })\n  effect.changes.push({\n    key: \"system.dynamicRollModifier.onYou.saves.agi\",\n    mode: 2,\n    value: `\"value\": 0, \"label\": \"Bane\", \"modifier\": \"- 1${dice}\"`\n  })\n  effect.changes.push({\n    key: \"system.dynamicRollModifier.onYou.saves.cha\",\n    mode: 2,\n    value: `\"value\": 0, \"label\": \"Bane\", \"modifier\": \"- 1${dice}\"`\n  })\n  effect.changes.push({\n    key: \"system.dynamicRollModifier.onYou.saves.int\",\n    mode: 2,\n    value: `\"value\": 0, \"label\": \"Bane\", \"modifier\": \"- 1${dice}\"`\n  })\n}",
        "trigger": "preItemRoll",
