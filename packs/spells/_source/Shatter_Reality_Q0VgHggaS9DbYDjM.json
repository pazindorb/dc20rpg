              "effectKey": "",
              "macro": "const roll = new Roll(\"1d4\", {});\nawait roll.evaluate();\nif (game.dice3d) await game.dice3d.showForRoll(roll, game.user, true, null, false);\n\nif (roll.total === 1) {\n  DC20.dialog.SimplePopup.info(\"Distorted Perception\", [\"It sees your body twist - you are Immobilized.\"]);\n  actor.toggleStatusEffect(\"immobilized\", {active: true, extras: {repeatedSave: true, repeatedSaveKey: \"cha\", against: event.against, id: \"immobilized\", forOneMinute: true}});\n}\nif (roll.total === 2) {\n  DC20.dialog.SimplePopup.info(\"Distorted Perception\", [\"Your senses collapse inward - you are Blinded.\"]);\n  actor.toggleStatusEffect(\"blinded\", {active: true, extras: {repeatedSave: true, repeatedSaveKey: \"cha\", against: event.against, id: \"blinded\", forOneMinute: true}});\n}\nif (roll.total === 3) {\n  DC20.dialog.SimplePopup.info(\"Distorted Perception\", [\"Your mind fractures - you are Stunned and can’t take Reactions.\"]);\n  actor.toggleStatusEffect(\"stunned\", {active: true, extras: {repeatedSave: true, repeatedSaveKey: \"cha\", against: event.against, id: \"stunned\", forOneMinute: true}});\n}\nif (roll.total === 4) {\n  DC20.dialog.SimplePopup.info(\"Distorted Perception\", [\"You become confused - At the start of each of your turns, you must spend 1 AP to move up to your Speed in a random direction.\"]);\n  const data = {\n    name: \"Distorted Perception\",\n    img: \"icons/magic/control/fear-fright-white.webp\",\n    system: {\n      duration: {\n        useCounter: true,\n        onTimeEnd: \"delete\"\n      },\n      macro: `\n      DC20.dialog.SimplePopup.info(\"Distorted Perception\", [\"You must spend 1 AP to move up to your Speed in a random direction.\"]);\n      `\n    },\n    duration: {rounds: 5},\n    changes: [\n      {\n        key: \"system.events\",\n        mode: 2,\n        value: `\"eventType\": \"macro\", \"trigger\": \"turnStart\", \"label\": \"Distorted Perception\", \"actorId\": \"${event.actorId}\"`\n      },\n      {\n        key: \"system.events\",\n        mode: 2,\n        value: `\"eventType\": \"saveRequest\", \"trigger\": \"turnEnd\", \"label\": \"Distorted Perception\", \"checkKey\": \"cha\", \"against\": \"${event.against}\", \"onSuccess\": \"delete\", \"actorId\": \"${event.actorId}\"`\n      },\n    ]\n  }\n  await actor.applyEffect(data)\n}\neffect.delete();",
