        "macro": "const debilitating = effect.flags.dc20rpg?.debilitating;\nconst deadly = effect.flags.dc20rpg?.deadly || 0;\nconst dmgValue = effect.flags.dc20rpg?.damageValue || 1;\n\n// We dont want to call it too fast as some tokens might be inside that template at the time we place it.\nconst skipTime = 5000 // 5 sec\nconst templateCallTime = effect.flags.dc20rpg?.templateCallTime || 0;\nconst effectCreationTime = effect._stats.createdTime\nif (event.trigger === \"instant\" && effectCreationTime - templateCallTime < skipTime) return;\n\n// When a creature enters the area for the first time on its TURN\nif (event.trigger === \"instant\" && !actor.myTurnActive) return;\n\nconst details = DC20.DC20Roll.prepareSaveDetails(\"mig\", {against: event.against, statuses: [\"poisoned\"], rollTitle: event.label});\nconst roll = await DC20.dialog.RollDialog.open(actor, details, {sendToActorOwners: true});\nif (roll && (roll._total < event.against)) {\n  let dmg = {\n    value: dmgValue,\n    source: \"Poison Cloud\",\n    type: \"poison\"\n  }\n  const calculationTarget = {\n    system: {damageReduction: actor.system.damageReduction}\n  }\n  dmg = game.dc20rpg.tools.calculateForTarget(calculationTarget, {clear: {...dmg}, modified: {...dmg}}, {isDamage: true})\n  game.dc20rpg.tools.applyDamage(actor, dmg.modified, {fromEvent: true});\n\n  // Apply Debilitating effect\n  if (debilitating) {\n    actor.toggleStatusEffect(debilitating, { active: true, extras: {forOneMinute: true, repeatedSave: true, repeatedSaveKey: \"mig\", against: event.against, id: debilitating } });\n  }\n\n  // Apply Deadly effect\n  if (deadly) {\n    const effectData = {\n      name: \"Deadly Poison\",\n      description: \"You take X Poison damage at the start of each of your turns.\",\n      img: \"icons/magic/unholy/projectile-smoke-tendril-green.webp\",\n      duration: {\n        rounds: 5\n      },\n      system: {\n        duration: {\n          useCounter: true,\n          onTimeEnd: \"delete\"\n        }\n      },\n      changes: [\n        {\n          key: \"system.events\",\n          mode: 2,\n          value: `\"eventType\": \"damage\", \"trigger\": \"turnStart\", \"label\": \"Deadly Poison\", \"value\": ${deadly}, \"type\": \"poison\", \"actorId\": \"${event.actorId}\"`\n        },\n        {\n          key: \"system.events\",\n          mode: 2,\n          value: `\"eventType\": \"saveRequest\", \"trigger\": \"turnEnd\", \"label\": \"Deadly Poison - Repeated Save\", \"checkKey\": \"mig\", \"against\": \"${event.against}\", \"statuses\": [\"poisoned\"], \"onSuccess\": \"delete\", \"actorId\": \"#SPEAKER_ID#\"`\n        },\n      ]\n    }\n    game.dc20rpg.effects.createEffectOn(effectData, actor);\n  }\n}",
