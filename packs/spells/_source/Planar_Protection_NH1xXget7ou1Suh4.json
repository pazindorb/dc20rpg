    "macros": {
      "BKvNQBuohgR4kwsL": {
        "command": "const effect = item.getEffectByKey(\"planarProtection\");\nif (!effect) return;\n\nconst MYSTIC_RESISTANCE = \"plBR3UAA4K08IfqR\";\nconst CONDITION_RESISTANCE = \"WcAQKfY8SFfAGdGE\";\n\nconst enhancements = item.enhancements.active;\nif (enhancements.get(MYSTIC_RESISTANCE)) {\n  effect.changes.push(  {\n    key: \"system.damageReduction.damageTypes.psychic.resistance\",\n    mode: 5,\n    value: true\n  });\n  effect.changes.push(  {\n    key: \"system.damageReduction.damageTypes.umbral.resistance\",\n    mode: 5,\n    value: true\n  });\n  effect.changes.push(  {\n    key: \"system.damageReduction.damageTypes.radiant.resistance\",\n    mode: 5,\n    value: true\n  });\n}\nelse {\n  effect.changes.push({\n    key: \"system.damageReduction.mdr.active\",\n    mode: 5,\n    value: true\n  })\n}\n\nconst options = {charmed: \"Charmed\", taunted: \"Taunted\", intimidated: \"Intimidated\"};\nconst selected = await DC20.dialog.SimplePopup.select(\"Select Condition\", options);\nif (selected) {\n  delete options[selected];\n  effect.changes.push({\n    key: `system.statusResistances.${selected}.resistance`,\n    mode: 2,\n    value: 1\n  });\n}\n\nif (enhancements.get(CONDITION_RESISTANCE)) {\n  const second = await DC20.dialog.SimplePopup.select(\"Select Second Condition\", options);\n  effect.changes.push({\n    key: `system.statusResistances.${second}.resistance`,\n    mode: 2,\n    value: 1\n  });\n}",
        "trigger": "preItemRoll",
