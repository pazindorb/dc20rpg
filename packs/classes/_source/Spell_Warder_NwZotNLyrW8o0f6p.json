    "macros": {
      "preItemRoll": "item.doNotSendToChat = true;\nlet alreadyActive = null;\nconst toSelect = {};\nitem.effects.forEach(effect => {\n  toSelect[effect.id]= effect.name;\n  if (actor.effects.getName(effect.name)) alreadyActive = actor.effects.getName(effect.name);\n});\nconst effectId = await game.dc20rpg.tools.getSimplePopup(\"select\", {header: \"Select Resitance Type\", selectOptions: toSelect});\nconst effect = item.effects.get(effectId);\nif (effect) {\n  if (alreadyActive) alreadyActive.delete();\n  game.dc20rpg.effectMacroHelper.addEffectToActor(effect, actor);\n}"
    }
