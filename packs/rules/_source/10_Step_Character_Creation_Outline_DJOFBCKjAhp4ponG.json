      "ownership": {
        "default": -1
      },
      "_key": "!journal.pages!DJOFBCKjAhp4ponG.ee9aa843908926ec"
    },
    {
      "_id": "3de9bc382a5b1549",
      "name": "Step 2: Save Masteries",
      "sort": 7,
      "text": {
        "content": "<p id=\"2be1b90a-e081-8027-8d33-c3b29e2b8394\">You add your Attribute bonus and your Combat Mastery together. The result is your Save bonus for each Attribute.</p>",
        "format": 1
      },
      "title": {
        "level": 2,
        "show": true
      },
      "type": "text",
      "system": {},
      "image": {},
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "category": null,
      "flags": {},
