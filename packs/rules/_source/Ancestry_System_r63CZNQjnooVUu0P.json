    {
      "_id": "08004d8effcb9aef",
      "name": "Gaining Ancestry Points",
      "ownership": {
        "default": -1
      },
      "sort": 3,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-814f-a513-ed2c73cd3df3\">When first creating your character, you have 5 Ancestry Points. You gain 2 additional Ancestry Points when you reach Level 4, and again when you reach Level 7.</p>",
        "format": 1
      },
      "title": {
        "level": 3,
        "show": true
      },
      "type": "text",
