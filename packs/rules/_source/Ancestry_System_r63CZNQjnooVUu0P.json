    {
      "_id": "822aa3a86c82629e",
      "name": "Minor Ancestry Traits",
      "ownership": {
        "default": -1
      },
      "sort": 5,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-8109-be11-f31bb7ee4deb\">These Traits cost 0 Ancestry Points and provide minor benefits that are respective of each Ancestry’s unique abilities and origins. You can only choose <strong>1</strong> Minor Trait.</p>",
        "format": 1
      },
      "title": {
        "level": 3,
        "show": true
      },
      "type": "text",
      "system": {},
      "image": {},
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "flags": {},
      "_stats": {
