    {
      "_id": "25237cdbd9d4639d",
      "name": "Variant Rules",
      "ownership": {
        "default": -1
      },
      "sort": 26,
      "text": {
        "content": "",
        "format": 1
      },
      "title": {
        "level": 1,
        "show": true
      },
      "type": "text",
      "system": {},
      "image": {},
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "flags": {},
      "_stats": {
        "compendiumSource": null,
        "duplicateSource": null,
        "coreVersion": "13.344",
        "systemId": "dc20rpg",
        "systemVersion": "0.9.0",
