      "_id": "2e9dfd919179ef7c",
      "name": "Ancestry Traits",
      "ownership": {
        "default": -1
      },
      "sort": 1,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-81a0-b3fa-e58bb07fd95c\">Ancestry Traits give your character physiological properties or abilities based on your Ancestry. You can choose various combinations of Ancestry Traits to customize your character to express the type of fantasy race you want to play.</p>",
        "format": 1
      },
      "title": {
        "level": 1,
        "show": true
      },
      "type": "text",
