    {
      "_id": "745f46dd8995f445",
      "name": "Ancestry Creation",
      "ownership": {
        "default": -1
      },
      "sort": 15,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-818a-995f-d9ce454f0abc\">Every character starts with the Base Ancestry Traits below, and their Ancestry gives them a list of additional Traits they can spend their <strong>5</strong> Ancestry Points on.</p>",
        "format": 1
      },
      "title": {
        "level": 1,
        "show": true
