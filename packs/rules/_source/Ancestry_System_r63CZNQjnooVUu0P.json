    {
      "_id": "7f2f16beed205ade",
      "name": "Types of Ancestry Traits",
      "ownership": {
        "default": -1
      },
      "sort": 4,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-817f-a6f1-f84b857e0a11\">Each Ancestry has 4 kinds of Ancestry Traits.</p>",
        "format": 1
      },
      "title": {
        "level": 2,
        "show": true
      },
      "type": "text",
      "system": {},
      "image": {},
      "video": {
