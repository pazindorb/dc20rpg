  "pages": [
    {
      "_id": "90ec09098dace566",
      "name": "Multiple Maneuvers",
      "ownership": {
        "default": -1
      },
      "sort": 1,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-8180-a547-cd5c2235e642\">You can't perform the same Maneuver more than once per Attack or Trigger, unless otherwise stated.</p>",
        "format": 1
      },
      "title": {
        "level": 3,
        "show": true
      },
      "type": "text",
      "system": {},
      "image": {},
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "flags": {},
