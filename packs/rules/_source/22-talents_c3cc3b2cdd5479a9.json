        {
            "_id": "d5108ae021d39684",
            "_key": "!journal.pages!c3cc3b2cdd5479a9.d5108ae021d39684",
            "_stats": {
                "compodiumSource": null,
                "coreVersion": "12.327",
                "createdTime": 1721371035.355543,
                "duplicateSource": null,
                "lastModifiedBy": "dOcXwrUs2VunZl0y",
                "modifiedTime": 1721371035.355543,
                "systemId": "dc20rpg",
                "systemVersion": "0.8.0"
            },
            "flags": {},
            "image": {},
            "name": "Adept Multiclass",
            "ownership": {
                "dOcXwrUs2VunZl0y": 3,
                "default": -1
            },
            "sort": 16,
            "src": null,
            "system": {},
