        "lastModifiedBy": null
      },
      "ownership": {
        "default": -1
      },
      "_key": "!journal.pages!WVbaBz9sb3cx12zI.c60f9c69d52fc9c7"
    },
    {
      "_id": "3e4712b1c6ab1e9a",
      "name": "MP Effects",
      "sort": 5,
      "text": {
        "content": "<p id=\"2be1b90a-e081-8086-b241-cfa644e7d509\">Any ability or effect that costs MP is considered an MP Effect. This includes Spells, Class Features, or Talents that you spend MP to use.</p>",
        "format": 1
      },
      "title": {
        "level": 3,
        "show": true
      },
      "type": "text",
      "system": {},
      "image": {},
      "video": {
