            "flags": {},
            "image": {},
            "name": "Hidden Creatures",
            "ownership": {
                "dOcXwrUs2VunZl0y": 3,
                "default": -1
            },
            "sort": 17,
            "src": null,
            "system": {},
            "text": {
                "content": "<p>Creatures that are Unseen or Unheard are more difficult to discover.</p>",
                "format": 1
            },
            "title": {
                "level": 2,
                "show": true
            },
            "type": "text",
