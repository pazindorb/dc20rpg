            "flags": {},
            "image": {},
            "name": "Unheard",
            "ownership": {
                "dOcXwrUs2VunZl0y": 3,
                "default": -1
            },
            "sort": 19,
            "src": null,
            "system": {},
            "text": {
                "content": "<p>You are Unheard while you remain silent, talk no louder than a whisper, or are within an area affected by the <em>Silence </em>Spell or a similar effect.</p>",
                "format": 1
            },
            "title": {
                "level": 3,
