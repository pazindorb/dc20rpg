    {
      "_id": "25a5c54b07df5a3f",
      "name": "Intimidated",
      "ownership": {
        "default": -1
      },
      "sort": 17,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-81aa-aced-e4d023b9803c\" class=\"block-color-default\">You have DisADV on all Checks while your source of intimidation is within your line of sight.</p>",
        "format": 1
      },
      "title": {
        "level": 3,
        "show": true
      },
      "type": "text",
      "system": {},
      "image": {},
      "video": {
