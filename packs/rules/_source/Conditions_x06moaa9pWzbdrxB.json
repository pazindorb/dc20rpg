    {
      "_id": "610e6b3221204f0a",
      "name": "Impaired",
      "ownership": {
        "default": -1
      },
      "sort": 15,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-8158-850b-e02cd119c685\">You have DisADV on Physical Checks.</p><p id=\"1cf1b90a-e081-810b-8acf-f1b5af8a94b7\"><strong>Heavily Impaired: </strong>You also have DisADV on Physical Saves.</p>",
        "format": 1
