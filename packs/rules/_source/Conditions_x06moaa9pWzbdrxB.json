      "_key": "!journal.pages!x06moaa9pWzbdrxB.26746480e81b89a9"
    },
    {
      "_id": "9257f5357f3153aa",
      "name": "Excluded Conditions",
      "ownership": {
        "default": -1
      },
      "sort": 4000000,
      "text": {
