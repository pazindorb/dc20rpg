    {
      "_id": "e49439b5f79839d3",
      "name": "Incapacitated",
      "ownership": {
        "default": -1
      },
      "sort": 16,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-8122-bd31-ccdfe45105f4\">You can’t speak, Concentrate, or spend Action Points.</p>",
        "format": 1
      },
      "title": {
        "level": 3,
        "show": true
      },
      "type": "text",
      "system": {},
      "image": {},
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "flags": {},
