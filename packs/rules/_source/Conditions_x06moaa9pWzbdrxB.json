    },
    {
      "_id": "2370179bf647d65e",
      "name": "Slowed",
      "ownership": {
        "default": -1
      },
      "sort": 25,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-81f6-bba0-e63c2c2c5dc9\">Every 1 Space you move costs an extra 1 Space of movement.</p>",
        "format": 1
      },
      "title": {
        "level": 3,
        "show": true
      },
      "type": "text",
      "system": {},
      "image": {},
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "flags": {},
