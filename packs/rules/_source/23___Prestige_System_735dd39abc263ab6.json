        {
            "_id": "5f417e9a35efc19f",
            "_key": "!journal.pages!735dd39abc263ab6.5f417e9a35efc19f",
            "_stats": {
                "compodiumSource": null,
                "coreVersion": "12.327",
                "createdTime": 1722088240.759506,
                "duplicateSource": null,
                "lastModifiedBy": "dOcXwrUs2VunZl0y",
