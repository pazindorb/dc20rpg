    {
      "_id": "0e1cdee2cebcbbb3",
      "name": "Multiclass Talents",
      "ownership": {
        "default": -1
      },
      "sort": 5,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-815c-8213-ddf654f853c7\">Multiclass Talents allow you to gain Features from other Classes, except Flavor Features (see Class Flavor Features below). The Features you take will be of lower level than your own Class, but this allows you to pick up powerful Features from other Classes and even Subclasses if you meet the requirements.</p>",
        "format": 1
      },
      "title": {
        "level": 1,
        "show": true
      },
      "type": "text",
      "system": {},
      "image": {},
      "video": {
        "controls": true,
        "volume": 0.5
      },
      "src": null,
      "flags": {},
      "_stats": {
        "compendiumSource": null,
        "duplicateSource": null,
        "coreVersion": "13.344",
        "systemId": "dc20rpg",
        "systemVersion": "0.9.0",
