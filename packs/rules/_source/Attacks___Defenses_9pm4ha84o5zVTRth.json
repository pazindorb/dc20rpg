    {
      "_id": "3b4db257b1613288",
      "name": "Attack Types",
      "ownership": {
        "default": -1
      },
      "sort": 2,
      "text": {
        "content": "<p id=\"1cf1b90a-e081-81a6-9906-f3e0c2eae39a\">There are 2 types of Attacks: <strong>Martial</strong> and <strong>Spell</strong>.</p>",
        "format": 1
      },
      "title": {
        "level": 2,
        "show": true
      },
      "type": "text",
      "system": {},
      "image": {},
      "video": {
