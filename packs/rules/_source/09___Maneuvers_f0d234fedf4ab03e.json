            },
            "flags": {},
            "image": {},
            "name": "Multiple Maneuvers",
            "ownership": {
                "dOcXwrUs2VunZl0y": 3,
                "default": -1
            },
            "sort": 2,
            "src": null,
            "system": {},
            "text": {
                "content": "<p>You can't perform the same Maneuver more than once per Attack Check or Trigger, unless otherwise stated.</p>",
                "format": 1
            },
            "title": {
                "level": 3,
                "show": true
            },
            "type": "text",
