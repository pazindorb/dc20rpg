    "pages": [
        {
            "_id": "8b4f8815f03795c6",
            "_key": "!journal.pages!f0d234fedf4ab03e.8b4f8815f03795c6",
            "_stats": {
                "compodiumSource": null,
                "coreVersion": "12.327",
                "createdTime": 1721459260.734043,
                "duplicateSource": null,
                "lastModifiedBy": "dOcXwrUs2VunZl0y",
